local Type = "Strength";
local NenKey = "N";

Luazifier.TextButton2.Activated:Connect(function()
	Type = Luazifier.TextButton2.Text;
	Luazifier.TextLabel1.Text = Type;
end)

Luazifier.TextButton3.Activated:Connect(function()
	Type = Luazifier.TextButton3.Text;
	Luazifier.TextLabel1.Text = Type;
end)

Luazifier.TextBox1.Changed:Connect(function()
	NenKey = Luazifier.TextBox1.Text;
end)

Luazifier.TextButton4.Activated:Connect(function()
	Type = Luazifier.TextButton4.Text;
	Luazifier.TextLabel1.Text = Type;
end)

Luazifier.TextButton5.Activated:Connect(function()
	Type = Luazifier.TextButton5.Text;
	Luazifier.TextLabel1.Text = Type;
end)

local bool = false
Luazifier.TextButton1.Activated:Connect(function()
	if bool then
		bool = false;
		Luazifier.Frame2.BackgroundColor3 = Color3.new(1, 0, 0);
	else
		bool = true;
		Luazifier.Frame2.BackgroundColor3 = Color3.new(0, 1, 0);
	end
end)


local player = game:GetService("Players").LocalPlayer;
local remote = player.Character.Character["input"];
local stam = player.Character.Humanoid.Info.Stamina;
local nen = player.Character.Humanoid.Info.NenStam;

function Meditate(val)
	if bool then
		if val then wait(val)end
		Nen(2);
		remote:FireServer("M");
		Hunger();
		repeat wait() until stam.Value == 100 or bool == false
		if bool then
			remote:FireServer("M");
			wait(0.5);
			Nen();
		end
	end
end		

function Nen(val)
    if nen.Value == 100 and NenKey ~= "" then
        remote:FireServer(NenKey);
        if val then wait(val) end
    end
end

function Hunger()
    if player.PlayerGui.CoreGUI.Images.Hunger.Size.X.Scale < 0.5 then
        local tool
        if player.Backpack:FindFirstChild("Meat") then
            tool = player.Backpack.Meat;
        elseif player.Backpack:FindFirstChild("Apple") then
            tool = player.Backpack.Apple;
        end
        if tool then
            player.Character.Humanoid:EquipTool(tool);
            tool:Activate();
            wait(0.5)
        end
    end
end

function Main()
    while true do
        if bool == true then
        	if Type == "Strength" then
    			remote:FireServer("J");
    			repeat wait() until player.PlayerGui:FindFirstChild("PushupsGui")
    			repeat wait() until	player.PlayerGui.PushupsGui.Pushups.BackgroundColor3 == Color3.new(0,0,0) or not player.PlayerGui:FindFirstChild("PushupsGui")
    			while player.PlayerGui:FindFirstChild("PushupsGui") and bool do
    			    local key = player.PlayerGui.PushupsGui.Pushups.Button.Text
    				player.PlayerGui.PushupsGui.Pushups.RemoteEvent:FireServer();
    				repeat wait() until not player.PlayerGui:FindFirstChild("PushupsGui") or player.PlayerGui.PushupsGui.Pushups.Button.Text ~= key
    				repeat wait() until not player.PlayerGui:FindFirstChild("PushupsGui") or player.PlayerGui.PushupsGui.Pushups.BackgroundColor3 == Color3.new(0,0,0)
    			end
    			if not bool then
    				remote:FireServer("J");
    			end
    			Meditate()
    		elseif Type == "Stamina" then
    			remote:FireServer("ClimbStart");
    			repeat wait() until stam.Value == 0 or bool == false
    			if not bool then
    				remote:FireServer("ClimbEnd");
    			end
    			Meditate(1)
    		elseif Type == "Run" then
    			remote:FireServer("FastSprintStart");
    			repeat wait() until stam.Value == 0 or bool == false
    			if not bool then
    				remote:FireServer("FastSprintEnd");
    			end
    			Meditate()
    		elseif Type == "Meditate" then
    		    if NenKey ~= "" then
        		        Nen(2);
        		        remote:FireServer("M");
        		        Hunger();
        		        while bool do
        		            if nen.Value == 100 then
                		        remote:FireServer("M");
                		        Nen(2);
                		        remote:FireServer("M");
                		        Hunger();
                		    else
                		        wait();
                		    end
            		    end
        	    else
        	        remote:FireServer("M");
        		    while bool do
        		        Hunger();
        		        wait();
    		        end
    		    end
    		end
        else
            wait();
        end
    end
end

Main();
